{
  "name": "metalsmith-simplewatch",
  "version": "0.0.0",
  "description": "A watch plugin for Metalsmith",
  "main": "./lib/",
  "scripts": {
    "test": "./node_modules/.bin/jshint ./lib/ ./test/ && ./node_modules/.bin/mocha --reporter spec"
  },
  "keywords": [
    "metalsmith",
    "watch"
  ],
  "author": "Christopher Cliff <ctcliff@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/christophercliff/metalsmith-simplewatch"
  },
  "license": "MIT",
  "dependencies": {
    "gaze": "^0.6.4"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.0.1",
    "jshint": "^2.5.2",
    "metalsmith": "^0.9.0",
    "mkdirp": "^0.5.0",
    "mocha": "^1.20.1",
    "rimraf": "^2.2.8"
  },
  "jshintConfig": {
    "eqeqeq": true,
    "immed": true,
    "latedef": "nofunc",
    "quotmark": "single",
    "trailing": true,
    "unused": true,
    "asi": true,
    "boss": true,
    "expr": true,
    "laxbreak": true,
    "laxcomma": true,
    "scripturl": true,
    "sub": true,
    "loopfunc": true
  }
}
